    "jsonfile": "^3.0.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "shelljs": "^0.7.8"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ],
