    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "scripts": {
    "dev": "next -H 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "css": "node-sass -r stylesheets -o static --precision 10",
    "watch_css": "nodemon -w stylesheets -e scss -x 'yarn run css'",
